# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           org.psjava.site.PsjavaSiteController.index()
GET     /ds/:id                           org.psjava.site.PsjavaSiteController.showDs(id:String)
GET     /algo/:id                           org.psjava.site.PsjavaSiteController.showAlgo(id:String)
GET     /assets/*file               controllers.Assets.at(path="/public", file)
GET /license org.psjava.site.PsjavaSiteController.license()
GET /clear-cache org.psjava.site.PsjavaSiteController.clearCache()
